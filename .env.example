# Lemon Squeezy API Configuration
# Get these from: https://app.lemonsqueezy.com/settings/api

# Your Lemon Squeezy API Key
LEMON_SQUEEZY_API_KEY=your_api_key_here

# Your Product ID (from Lemon Squeezy product settings)
LEMON_SQUEEZY_PRODUCT_ID=your_product_id_here

# Webhook Secret (for verifying webhook signatures)
LEMON_SQUEEZY_WEBHOOK_SECRET=your_webhook_secret_here

# App URL (for CORS and redirects)
VITE_APP_URL=http://localhost:5173

# API URL (for license validation)
VITE_API_URL=http://localhost:3000/api
